{
	"root": true,
	"extends": ["plugin:react/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"quotes": [
			"error",
			"single",
			{
				"allowTemplateLiterals": true
			}
		],
		"semi": ["error", "never"],
		"space-before-function-paren": 0,
		"multiline-ternary": 0,
		"no-unused-vars": 1
	}
}
